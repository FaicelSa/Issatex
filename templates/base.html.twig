        {% block javascripts %}
            {# {{ encore_entry_script_tags('app') }} #}
        {% endblock %}
    </head>
    <body>
    <h1 class="text-center">{% block titre %} bienvenue à mon premier exemple{% endblock %}</h1>
        {% block body %}{% endblock %}
       
    </body>
